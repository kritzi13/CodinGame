#include <iostream>
#include <string>
#include <vector>
#include <algorithm>

using namespace std;
int main()
{
    while (1) {    
        int max_h=0;
        int imax_h=0;

        for (int i = 0; i < 8; i++) {
            int mountain_h; // represents the height of one mountain.
            cin >> mountain_h; cin.ignore();
            if (mountain_h>max_h){
                max_h=mountain_h;
                imax_h=i;
            }
        }
        cout<<imax_h<<endl;
    }
}
